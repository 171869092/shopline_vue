    "core-js": "3.6.5",
    "echarts": "4.2.1",
    "element-ui": "2.15.0",
    "html2canvas": "^1.0.0-rc.7",
    "js-cookie": "2.2.0",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.3.1",
    "lodash": "^4.17.20",
    "normalize.css": "7.0.0",
    "path-to-regexp": "2.4.0",
    "swiper": "^5.4.5",
    "vue": "2.6.10",
    "vue-awesome-swiper": "^4.1.1",
    "vue-count-to": "^1.0.13",
    "vue-facebook-signin-button": "^1.0.1",
    "vue-google-signin-button-directive": "^1.0.3",
